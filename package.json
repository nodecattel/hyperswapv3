{
    "name": "hyperswap-v3",
    "version": "1.0.0",
    "description": "HyperSwap V3 Grid Trading Bot - Professional TypeScript grid trading system for HyperLiquid",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
      "build": "tsc",
      "build:watch": "tsc --watch",
      "dev": "ts-node index.ts",
      "start": "npm run build && node dist/index.js",
      "start:dev": "ts-node index.ts",
      "bot": "npm run start",
      "test": "ts-node src/cli/gridCli.ts test",
      "test:build": "npm run build && node dist/cli/gridCli.js test",
      "test:swap": "npm run build && node scripts/testSwapExecution.js",
      "test:swap:dev": "ts-node scripts/testSwapExecution.js",
      "test:pricing": "npm run build && node scripts/testMultiPairPricing.js",
      "test:pricing:dev": "ts-node scripts/testMultiPairPricing.js",
      "config": "ts-node src/cli/gridCli.ts config",
      "status": "ts-node src/cli/gridCli.ts status",
      "stop": "ts-node src/cli/gridCli.ts stop",
      "dashboard": "ts-node src/dashboard/server.ts",
      "dashboard:build": "npm run build && node dist/dashboard/server.js",
      "grid": "ts-node src/cli/gridCli.ts",
      "grid:config": "ts-node src/cli/gridCli.ts config",
      "grid:start": "ts-node src/cli/gridCli.ts start",
      "grid:stop": "ts-node src/cli/gridCli.ts stop",
      "grid:status": "ts-node src/cli/gridCli.ts status --",
      "grid:watch": "ts-node src/cli/gridCli.ts status --watch",
      "grid:test": "ts-node src/cli/gridCli.ts test",
      "clean": "rm -rf dist",
      "type-check": "tsc --noEmit",
      "lint": "tsc --noEmit --strict"
    },
    "dependencies": {
      "dotenv": "^16.0.3",
      "@ethersproject/abi": "^5.7.0",
      "@ethersproject/contracts": "^5.7.0",
      "ethers": "^5.7.0",
      "inquirer": "^8.2.5",
      "chalk": "^4.1.2",
      "commander": "^9.4.1",
      "ora": "^5.4.1",
      "boxen": "^5.1.2",
      "figlet": "^1.5.2",
      "express": "^4.18.2",
      "socket.io": "^4.7.2",
      "cors": "^2.8.5",
      "axios": "^1.4.0",
      "winston": "^3.8.2",
      "node-cron": "^3.0.2",
      "ws": "^8.14.2"
    },
    "devDependencies": {
      "typescript": "^5.2.2",
      "ts-node": "^10.9.1",
      "@types/node": "^20.8.0",
      "@types/inquirer": "^9.0.3",
      "@types/figlet": "^1.5.6",
      "@types/express": "^4.17.17",
      "@types/cors": "^2.8.14",
      "@types/ws": "^8.5.6",
      "@types/winston": "^2.4.4",
      "nodemon": "^3.0.1",
      "concurrently": "^8.2.1"
    },
    "keywords": ["dex", "v3", "hyperswap", "blockchain"],
    "author": "",
    "license": "ISC"
  }
  